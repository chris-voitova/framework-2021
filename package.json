    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.10",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
